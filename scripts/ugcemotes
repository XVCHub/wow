local Unknown1 = {}
local Unknown2 = {}
local Unknown3 = {}
local Unknown4 = {}
local AllValue = 'all'
local NilValue1 = nil
local NilValue2 = nil
local OneValue = 1
local FalseValue1 = false
local FalseValue2 = false
local EmotesData = {
    Label1,
    Frame1,
    InputField,
    EmotesList,
    Frame3,
    Frame2,
}

local function Func1()
    pcall(function()
        if writefile then
            local PairKey, PairValue, PairIndex = pairs(Unknown2)
            local JsonValue = {}

            while true do
                local UnknownValue

                PairIndex, UnknownValue = PairKey(PairValue, PairIndex)

                if PairIndex == nil then
                    break
                end
                if UnknownValue then
                    JsonValue[tostring(PairIndex)] = true
                end
            end

            local EncodedJson = HttpService:JSONEncode(JsonValue)

            writefile('ugc_emotes_favorites.json', EncodedJson)
        end
    end)
end
local function Func2()
    pcall(function()
        if writefile then
            local DecodedJson = HttpService:JSONEncode(Unknown4)

            writefile('ugc_emotes_custom.json', DecodedJson)
        end
    end)
end
local function Func3()
    pcall(function()
        if readfile and (isfile and isfile('ugc_emotes_favorites.json')) then
            local FavoritesJson = readfile('ugc_emotes_favorites.json')

            if FavoritesJson and (FavoritesJson ~= '' and FavoritesJson ~= 'null') then
                local DecodedFavorites, FavoritesData = pcall(function()
                    return HttpService:JSONDecode(FavoritesJson)
                end)

                if DecodedFavorites and type(FavoritesData) == 'table' then
                    Unknown2 = {}

                    local FavoritesKey, FavoritesValue, FavoritesIndex = pairs(FavoritesData)

                    while true do
                        local UnknownData

                        FavoritesIndex, UnknownData = FavoritesKey(FavoritesValue, FavoritesIndex)

                        if FavoritesIndex == nil then
                            break
                        end
                        if UnknownData then
                            Unknown2[tonumber(FavoritesIndex) or FavoritesIndex] = true
                        end
                    end
                end
            end
        end
    end)
end
local function Func4()
    pcall(function()
        if readfile and (isfile and isfile('ugc_emotes_custom.json')) then
            local CustomJson = readfile('ugc_emotes_custom.json')

            if CustomJson and (CustomJson ~= '' and CustomJson ~= 'null') then
                local DecodedCustom, CustomData = pcall(function()
                    return HttpService:JSONDecode(CustomJson)
                end)

                if DecodedCustom and type(CustomData) == 'table' then
                    Unknown4 = CustomData
                end
            end
        end
    end)
end

Func3()
Func4()

local function Func5(Param1, Param2)
    local CharacterModel = LocalPlayer.Character
    local CharacterModel1 = LocalPlayer.Character

    if CharacterModel1 then
        CharacterModel1 = LocalPlayer.Character:FindFirstChild('Humanoid')
    end
    if CharacterModel and CharacterModel1 then
        if NilValue1 ~= Param1 then
            local TrackKey, TrackValue, TrackIndex = pairs(CharacterModel1:GetPlayingAnimationTracks())

            while true do
                local UnknownTrack

                TrackIndex, UnknownTrack = TrackKey(TrackValue, TrackIndex)

                if TrackIndex == nil then
                    break
                end
                if UnknownTrack.Priority == Enum.AnimationPriority.Action then
                    UnknownTrack:Stop()
                end
            end

            pcall(function()
                local AnimationAsset = Instance.new('Animation')

                AnimationAsset.AnimationId = 'rbxassetid://' .. Param1

                local LoadedAnimation = CharacterModel1:LoadAnimation(AnimationAsset)

                LoadedAnimation.Priority = Enum.AnimationPriority.Action
                LoadedAnimation.Looped = true

                LoadedAnimation:Play()
                LoadedAnimation:AdjustSpeed(OneValue)

                LoadedAnimation.TimePosition = 0.1
                NilValue1 = Param1
                NilValue2 = LoadedAnimation
                Label1.Text = 'Playing: ' .. Param2 .. ' (' .. string.format('%.2fx', OneValue) .. ')'
                Label1.TextColor3 = Color3.new(0.5, 1, 0.5)

                spawn(function()
                    wait(2)

                    Label1.Text = 'Ready'
                    Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
                end)
                AnimationAsset:Destroy()
            end)
        else
            local PlayingTrack, TrackName, TrackTime = pairs(CharacterModel1:GetPlayingAnimationTracks())

            while true do
                local UnknownTime

                TrackTime, UnknownTime = PlayingTrack(TrackName, TrackTime)

                if TrackTime == nil then
                    break
                end
                if UnknownTime.Priority == Enum.AnimationPriority.Action then
                    UnknownTime:Stop()
                end
            end

            NilValue1 = nil
            NilValue2 = nil
            Label1.Text = 'Stopped: ' .. Param2
            Label1.TextColor3 = Color3.new(1, 1, 0.5)

            spawn(function()
                wait(2)

                Label1.Text = 'Ready'
                Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
            end)
        end
    else
        Label1.Text = 'No character'
        Label1.TextColor3 = Color3.new(1, 0.5, 0.5)

        return
    end
end
local function Func6(Param3, Param4)
    local Frame6 = Instance.new('Frame')

    Frame6.Size = UDim2.new(1, 0, 0, 35)
    Frame6.BackgroundTransparency = 1
    Frame6.Parent = EmotesList

    local Dimension = Param4 and UDim2.new(1, -100, 1, 0) or UDim2.new(1, -70, 1, 0)
    local Button7 = Instance.new('TextButton')

    Button7.Size = Dimension
    Button7.Position = UDim2.new(0, 0, 0, 0)
    Button7.BackgroundColor3 = Color3.new(0, 0, 0)
    Button7.BackgroundTransparency = 0.5
    Button7.Text = ' ' .. Param3.name
    Button7.TextColor3 = Color3.new(1, 1, 1)
    Button7.TextSize = 12
    Button7.Font = Enum.Font.Gotham
    Button7.TextXAlignment = Enum.TextXAlignment.Left
    Button7.BorderSizePixel = 0
    Button7.Parent = Frame6

    local Corner13 = Instance.new('UICorner')

    Corner13.CornerRadius = UDim.new(0, 12)
    Corner13.Parent = Button7

    local Button8 = Instance.new('TextButton')

    Button8.Size = UDim2.new(0, 32, 1, 0)
    Button8.Position = UDim2.new(1, Param4 and -98 or -66, 0, 0)
    Button8.BackgroundTransparency = 1
    Button8.Text = Unknown2[Param3.id] and '\u{2605}' or '\u{2606}'
    Button8.TextColor3 = Unknown2[Param3.id] and Color3.new(1, 0.8, 0) or Color3.new(0.7, 0.7, 0.7)
    Button8.TextSize = 16
    Button8.BorderSizePixel = 0
    Button8.Parent = Frame6

    local Button9 = Instance.new('TextButton')

    Button9.Size = UDim2.new(0, 32, 1, 0)
    Button9.Position = UDim2.new(1, Param4 and -64 or -32, 0, 0)
    Button9.BackgroundTransparency = 1
    Button9.Text = Unknown3[Param3.id] and (Unknown3[Param3.id].Name:gsub('KeyCode%.', ''):sub(1, 3) or 'Bind') or 'Bind'
    Button9.TextColor3 = Color3.new(0.8, 0.8, 0.8)
    Button9.TextSize = 8
    Button9.Font = Enum.Font.Gotham
    Button9.BorderSizePixel = 0
    Button9.Parent = Frame6

    local UnknownValue1

    if Param4 then
        UnknownValue1 = Instance.new('TextButton')
        UnknownValue1.Size = UDim2.new(0, 30, 1, 0)
        UnknownValue1.Position = UDim2.new(1, -30, 0, 0)
        UnknownValue1.BackgroundColor3 = Color3.new(0.5, 0, 0)
        UnknownValue1.BackgroundTransparency = 0.3
        UnknownValue1.Text = 'Del'
        UnknownValue1.TextColor3 = Color3.new(1, 1, 1)
        UnknownValue1.TextSize = 8
        UnknownValue1.Font = Enum.Font.Gotham
        UnknownValue1.BorderSizePixel = 0
        UnknownValue1.Parent = Frame6

        local CornerInstance = Instance.new('UICorner')

        CornerInstance.CornerRadius = UDim.new(0, 12)
        CornerInstance.Parent = UnknownValue1
    else
        UnknownValue1 = nil
    end

    Button7.MouseEnter:Connect(function()
        Button7.BackgroundTransparency = 0.3
    end)
    Button7.MouseLeave:Connect(function()
        Button7.BackgroundTransparency = 0.5
    end)
    Button7.MouseButton1Click:Connect(function()
        Func5(Param3.id, Param3.name)
    end)
    Button8.MouseButton1Click:Connect(function()
        if Unknown2[Param3.id] then
            Unknown2[Param3.id] = nil
            Button8.Text = '\u{2606}'
            Button8.TextColor3 = Color3.new(0.7, 0.7, 0.7)
        else
            Unknown2[Param3.id] = true
            Button8.Text = '\u{2605}'
            Button8.TextColor3 = Color3.new(1, 0.8, 0)
        end

        Func1()

        if AllValue == 'favorites' then
            spawn(function()
                wait(0.1)
                loadGUI()
            end)
        end
    end)
    Button9.MouseButton1Click:Connect(function()
        Label1.Text = 'Press any key to bind...'
        Label1.TextColor3 = Color3.new(1, 1, 0.5)

        local UnknownVariable1 = nil

        UnknownVariable1 = InputService.InputBegan:Connect(function(Parameter2, Parameter3)
            if not Parameter3 then
                if Parameter2.KeyCode ~= Enum.KeyCode.Unknown then
                    Unknown3[Param3.id] = Parameter2.KeyCode
                    Button9.Text = Parameter2.KeyCode.Name:gsub('KeyCode%.', ''):sub(1, 3)
                    Button9.TextColor3 = Color3.new(1, 1, 1)
                    Label1.Text = 'Bound to ' .. Parameter2.KeyCode.Name:gsub('KeyCode%.', '')
                    Label1.TextColor3 = Color3.new(0.5, 1, 0.5)

                    spawn(function()
                        wait(2)

                        Label1.Text = 'Ready'
                        Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
                    end)
                    UnknownVariable1:Disconnect()
                end
            end
        end)
    end)

    if UnknownValue1 then
        UnknownValue1.MouseEnter:Connect(function()
            UnknownValue1.BackgroundTransparency = 0.1
        end)
        UnknownValue1.MouseLeave:Connect(function()
            UnknownValue1.BackgroundTransparency = 0.3
        end)
        UnknownValue1.MouseButton1Click:Connect(function()
            local Value4, Value5, Value6 = pairs(Unknown4)

            while true do
                local UnusedValue7

                Value6, UnusedValue7 = Value4(Value5, Value6)

                if Value6 == nil then
                    break
                end
                if UnusedValue7.id == Param3.id and UnusedValue7.name == Param3.name then
                    table.remove(Unknown4, Value6)

                    break
                end
            end

            Func2()
            loadGUI()

            Label1.Text = 'Deleted: ' .. Param3.name
            Label1.TextColor3 = Color3.new(1, 1, 0.5)

            spawn(function()
                wait(2)

                Label1.Text = 'Ready'
                Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
            end)
        end)
    end
end

function loadGUI()
    local ScrollingFrameInstance = EmotesList
    local ChildInstance1, ChildInstance2, ChildInstance3 = pairs(ScrollingFrameInstance:GetChildren())

    while true do
        local UnusedValue13

        ChildInstance3, UnusedValue13 = ChildInstance1(ChildInstance2, ChildInstance3)

        if ChildInstance3 == nil then
            break
        end
        if UnusedValue13:IsA('Frame') then
            UnusedValue13:Destroy()
        end
    end

    local TableInstance = {}
    local TextBoxText = InputField.Text:lower()

    Frame2.Visible = AllValue == 'custom'

    if AllValue ~= 'custom' then
        EmotesList.Size = UDim2.new(1, -16, 1, -140)
        EmotesList.Position = UDim2.new(0, 8, 0, 105)
    else
        EmotesList.Size = UDim2.new(1, -16, 1, -200)
        EmotesList.Position = UDim2.new(0, 8, 0, 160)
    end
    if AllValue ~= 'all' then
        if AllValue ~= 'favorites' then
            if AllValue == 'custom' then
                local Value16, Value17, Value18 = pairs(Unknown4)

                while true do
                    local UnusedValue19

                    Value18, UnusedValue19 = Value16(Value17, Value18)

                    if Value18 == nil then
                        break
                    end
                    if TextBoxText == '' or UnusedValue19.name:lower():find(TextBoxText) then
                        table.insert(TableInstance, UnusedValue19)
                    end
                end
            end
        else
            local Value20, Value21, Value22 = pairs(Unknown1)

            while true do
                local UnusedValue23

                Value22, UnusedValue23 = Value20(Value21, Value22)

                if Value22 == nil then
                    break
                end
                if Unknown2[UnusedValue23.id] and (TextBoxText == '' or UnusedValue23.name:lower():find(TextBoxText)) then
                    table.insert(TableInstance, UnusedValue23)
                end
            end
        end
    else
        local Value24, Value25, Value26 = pairs(Unknown1)

        while true do
            local UnusedValue27

            Value26, UnusedValue27 = Value24(Value25, Value26)

            if Value26 == nil then
                break
            end
            if TextBoxText == '' or UnusedValue27.name:lower():find(TextBoxText) then
                table.insert(TableInstance, UnusedValue27)
            end
        end
    end

    local TableValue1, TableValue2, TableValue3 = pairs(TableInstance)

    while true do
        local UnusedValue31

        TableValue3, UnusedValue31 = TableValue1(TableValue2, TableValue3)

        if TableValue3 == nil then
            break
        end

        Func6(UnusedValue31, AllValue == 'custom')
    end

    spawn(function()
        wait(0.1)

        EmotesList.CanvasSize = UDim2.new(0, 0, 0, EmotesLayout.AbsoluteContentSize.Y)
    end)
end

