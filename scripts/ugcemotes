Button5.MouseButton1Click:Connect(function()
    local TrimmedText1 = InputField1.Text:match('^%s*(.-)%s*$')
    local TrimmedText2 = InputField2.Text:match('^%s*(.-)%s*$')

    if TrimmedText1 == '' or TrimmedText2 == '' then
        Label1.Text = 'Please fill both fields'
        Label1.TextColor3 = Color3.new(1, 0.5, 0.5)

        spawn(function()
            wait(2)

            Label1.Text = 'Ready'
            Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
        end)

        return
    else
        local NumericValue = tonumber(TrimmedText2)

        if NumericValue then
            local Value36, Value37, Value38 = pairs(Unknown4)

            while true do
                local UnusedValue39

                Value38, UnusedValue39 = Value36(Value37, Value38)

                if Value38 == nil then
                    break
                end
                if UnusedValue39.id == NumericValue then
                    Label1.Text = 'Animation ID already exists'
                    Label1.TextColor3 = Color3.new(1, 0.5, 0.5)

                    spawn(function()
                        wait(2)

                        Label1.Text = 'Ready'
                        Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
                    end)

                    return
                end
            end

            table.insert(Unknown4, {
                id = NumericValue,
                name = TrimmedText1,
            })
            Func2()

            InputField1.Text = ''
            InputField2.Text = ''

            loadGUI()

            Label1.Text = 'Added: ' .. TrimmedText1
            Label1.TextColor3 = Color3.new(0.5, 1, 0.5)

            spawn(function()
                wait(2)

                Label1.Text = 'Ready'
                Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
            end)
        else
            Label1.Text = 'Invalid animation ID'
            Label1.TextColor3 = Color3.new(1, 0.5, 0.5)

            spawn(function()
                wait(2)

                Label1.Text = 'Ready'
                Label1.TextColor3 = Color3.new(0.7, 0.7, 0.7)
            end)
        end
    end
end)
