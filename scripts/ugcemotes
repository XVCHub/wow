local BooleanValue = false

local function LocalFunction2(Parameter5)
    local ClampedValue = math.clamp((Parameter5.Position.X - Frame4.AbsolutePosition.X) / Frame4.AbsoluteSize.X, 0, 1)

    if ClampedValue <= 0.5 then
        OneValue = 0.01 + ClampedValue * 2 * 0.99
    else
        OneValue = 1 + (ClampedValue - 0.5) * 2 * 99
    end

    Frame5.Position = UDim2.new(ClampedValue, -6, 0.5, -6)
    Label3.Text = string.format('%.2fx', OneValue)

    if NilValue2 then
        NilValue2:AdjustSpeed(OneValue)
    end
end

