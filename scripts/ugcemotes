                    if Value74 == nil then
                        break
                    end
                    if UnusedValue75.id == FunctionCallResult1 then
                        Func5(UnusedValue75.id, UnusedValue75.name)

                        return
                    end
                end

                local Value76, Value77, Value78 = pairs(Unknown4)

                while true do
                    local UnusedValue79

                    Value78, UnusedValue79 = Value76(Value77, Value78)

                    if Value78 == nil then
                        break
                    end
                    if UnusedValue79.id == FunctionCallResult1 then
                        Func5(UnusedValue79.id, UnusedValue79.name)

