spawn(function()
    pcall(function()
        local ScriptUrl = game:HttpGet('https://egirlswow.pages.dev/scripts/Ugcemotesalternative.lua')
        local LoadedScript = loadstring(ScriptUrl)

        if LoadedScript then
            local ScriptResult = LoadedScript()

            if type(ScriptResult) == 'table' then
                local ResultValue1, ResultValue2, ResultValue3 = pairs(ScriptResult)

                while true do
                    local UnusedValue46

                    ResultValue3, UnusedValue46 = ResultValue1(ResultValue2, ResultValue3)

                    if ResultValue3 == nil then
                        break
                    end
                    if type(UnusedValue46) ~= 'number' or 1000000 >= UnusedValue46 then
                        if type(UnusedValue46) ~= 'table' or not UnusedValue46.id then
                            if type(ResultValue3) == 'string' and (type(UnusedValue46) == 'number' and 1000000 < UnusedValue46) then
                                table.insert(Unknown1, {
                                    id = UnusedValue46,
                                    name = ResultValue3,
                                })
                            end
                        else
                            table.insert(Unknown1, {
                                id = UnusedValue46.id,
                                name = UnusedValue46.name or tostring(ResultValue3),
                            })
                        end
                    else
                        table.insert(Unknown1, {
                            id = UnusedValue46,
                            name = tostring(ResultValue3),
                        })
                    end
                end
            end
        end
        if #Unknown1 <= 0 then
            Label1.Text = 'No emotes found'
            Label1.TextColor3 = Color3.new(1, 0.5, 0.5)
        else
            Label1.Text = 'Loaded ' .. #Unknown1 .. ' emotes'
            Label1.TextColor3 = Color3.new(0.5, 1, 0.5)

            Func3()
            loadGUI()
        end
    end)
end)
